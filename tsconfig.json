{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "paths": {
      "baseUrl": "./",
      "@app/*": [
        "src/app/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@features/*": [
        "src/features/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@processes/*": [
        "src/processes/*"
      ],
      "@shared/*": [
        "src/shared/*"
      ],
      "@widgets/*": [
        "src/widgets/*"
      ]
    }
  }
}
